        function SetBarNumber(BarNumber) {
            if (BarNumber != "" && BarNumber != "undefined" && BarNumber != undefined) {
                $.get("https://www.browardclerk.org//Web2/Services/SetBarNumber?BarNumber=" + BarNumber, function (data) {

