        function guid() {
            function s4() {
                return Math.floor((1 + Math.random()) * 0x10000)
                    .toString(16)
                    .substring(1);
            }

            return s4() + s4() + '-' + s4() + '-' + s4() + '-' +
                s4() + '-' + s4() + s4() + s4();
        }

        function getSessionGuid() {
            var SessionGUID = localStorage.getItem('SessionGUID');
            if (!SessionGUID) {
                SessionGUID = guid();
                localStorage.setItem('SessionGUID', SessionGUID);
                $("#SessionGUID").val(SessionGUID);
            }
            return SessionGUID;
        }

        function UpdateCartCount() {

            $.post('https://www.browardclerk.org//Web2/ShoppingCart/GetCartCount/?sessionGUID=' + getSessionGuid(), {},
            function (data, status) {
                var CartCount;
                if (data != "Error") {
                    if (parseInt(JSON.parse(JSON.parse(data)))) {
                        CartCount = parseInt(JSON.parse(data));
                        $('#liCart').removeClass('hidden').show();
                        $('#gapCart').removeClass('hidden').show();
                    } else {
                        $('#liCart').addClass('hidden').hide();
                        $('#gapCart').addClass('hidden').hide();
                        CartCount = 0;
                    }
                    $('#lblCartCount').html(CartCount);
                } else {
                    toastr.error("Some error happened while deleting this item.");
                }
            }).done(function () {
