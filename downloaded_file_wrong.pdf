    <!--Redirect for external URLs-->
    <script>
        $(window).on('load', function () {
            $.expr[':'].external = function (a) {
                var PATTERN_FOR_EXTERNAL_URLS = /^(\w+:)?\/\//;
                var href = $(a).attr('href');
                if (href !== undefined && href.toUpperCase().indexOf('.BROWARDCLERK.') == -1 && href.toUpperCase().indexOf('LOCALHOST') == -1) {
                    return href !== undefined && href.search(PATTERN_FOR_EXTERNAL_URLS) !== -1;
                }
            };
            $("a:external").addClass("ext_link");
            $(function () {
                $('a.ext_link').click(function (e) {
                    var RedirectURL = $(this).attr('href');
                    var CurrentPageURL = window.location.href;
                    $.ajax({
                        type: 'POST',
                        url: '../Home/ExternalRedirectSession',
                        data: { 'redirectURL': RedirectURL, 'currentpageURL': CurrentPageURL },
                        success: function (response)
                        {
                            if (response == true)
                            {
                                window.open('/Web2/Home/ExternalRedirect', "_blank");
                            }
                            else
                                if (response == error)
                                {
                                    window.open('/Web2/Home', "_blank");
                                }
                        }
                    })
                    return false;
                });

            });
        });
    </script>


    <!-- BEGIN timeout alerts here -->
    <script type="text/javascript">

        var timeOutActive =  true;
        var logoutUrl = 'https://www.browardclerk.org//Web2/Account/LogOut/?reason=sessiontimeout';

        var sessionTimeoutTimer


        function CheckSession() {

            var sUserID = '';


            if (!$('#loginForm').length )
            {

                if (timeOutActive == true) {
                    var url = '/Web2/Session/GetRemainingSessionTimeout';
                    $.ajax({
                        type: "POST",
                        url: url,
                        data: {},
                        success: function (result) {

                            //alert('Session:'+ result.SessionTimeoutMinutes + '   Warning:' + result.SessionTimeWarningMinutes + '   Remaining:' + result.RemainingSessionTimeMinutes)
                            //$('#sessionButton').attr('value', 'Session:'+ result.SessionTimeoutMinutes + '   Warning:' + result.SessionTimeWarningMinutes + '   Remaining:' + result.RemainingSessionTimeMinutes);

                            sessionTimeoutTimer = setTimeout("CheckSession()", ((result.RemainingSessionTimeMinutes - result.SessionTimeWarningMinutes) * 60000) + 1000);

                            if (result.RemainingSessionTimeMinutes == 0)
                            {

                                if (sUserID == "")
                                {
                                    window.location = "https://www.browardclerk.org"; //Clerk Home Page
                                }
                                else
                                {
                                    window.location = logoutUrl;
                                }

                            }

                            if ( result.RemainingSessionTimeMinutes <= result.SessionTimeWarningMinutes)
                            {
                                $('#timeout').modal('show');
                            }
                            else
                            {
                                $('#timeout').modal('hide');
                            }

                        },

                        error: function (result) {
                            // alert("Error");
                        }
                    });

                }

            }

        };


    </script>
